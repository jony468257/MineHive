<script>
    $('.language-selection').change(function () {
        let len = $(this).val();
        axios.put('{{ route('language.change') }}', {
            len: len
        }).then((response) => {
            if(response.data === 1){
                window.relo
            }
            console.log(response.data)
        })
    })
</script>
